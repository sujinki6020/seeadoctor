<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.seeadoctor.repository.mapper.SampleMapper">
	

    <select id="selectSample" parameterType="sample" resultType="sample">
	    select *
	    from tb_test
		
    </select>
    	
   <insert id="insertSample" parameterType="sample">
		insert into tb_test(
			no, content
		) values (
			#{no}, #{content}
		)
   </insert>
   
  
</mapper>






















